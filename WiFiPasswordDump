(set output=wifiOutput.txt & echo ======WiFi Key Content on %computername%====== > %output% & netsh wlan show profile * | for /f "delims=':' tokens=1*" %A in ('findstr /i /C:"ssid name"') do (echo Key Content for %B >> %output% & netsh wlan show profile %B key=clear | for /f "delims=':' tokens=1*" %E in ('findstr /i /C:"key content"') do (echo %F >> %output% & echo. >> %output%) ) ) &echo ============================================= >> %output% & %output%
