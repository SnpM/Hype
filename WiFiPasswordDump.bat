echo ======WiFi Key Content on %computername%====== > wifiOutput.txt & (netsh wlan show profile * | for /f "delims=':' tokens=1*" %%A in ('findstr /i /C:"ssid name"') do (echo Key Content for %%B >> wifiOutput.txt & (netsh wlan show profile %%B key=clear | for /f "delims=':' tokens=1*" %%E in ('findstr /i /C:"key content"') do (echo %%F >> wifiOutput.txt)) & echo. >> wifiOutput.txt ) ) &echo ============================================= >> wifiOutput.txt & notepad.exe wifiOutput.txt
